# Anthropic API key for Claude Agent SDK
ANTHROPIC_API_KEY=your-api-key-here

# Composio API key for Tool Router
COMPOSIO_API_KEY=your-composio-api-key-here

# Smithery API key (optional) for MCP tools via Smithery Connect (e.g. Exa search)
SMITHERY_API_KEY=

# DataForSEO credentials (optional) for SEO tools via DataForSEO MCP servers
DATAFORSEO_USERNAME=
DATAFORSEO_PASSWORD=

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# OpenAI (for text-embedding-3-small vector embeddings)
OPENAI_API_KEY=your-openai-api-key-here

# Browser automation (optional) â€” also configurable in Settings UI
# BROWSER_ENABLED=false        # Enable browser automation tools
# BROWSER_MODE=clawd            # 'clawd' (isolated Chromium) or 'chrome' (CDP)
# BROWSER_HEADLESS=false        # Run browser without visible window
# BROWSER_BACKEND=builtin       # 'builtin' (Playwright) or 'agent-browser' (Vercel)

# Set to false once migration from localStorage is confirmed
ALLOW_ANONYMOUS=true
